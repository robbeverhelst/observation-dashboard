{
  "name": "observation-explorer",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "bun run redis:start && next dev",
    "dev:no-redis": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next node_modules/.cache .turbo",
    "ci": "bun run format:check && bun run lint && bun run build",
    "redis:start": "docker-compose up -d redis 2>/dev/null || echo 'Redis already running or starting...'",
    "redis:stop": "docker-compose stop redis",
    "redis:restart": "bun run redis:stop && bun run redis:start",
    "docker:build": "docker build -t observation-explorer .",
    "docker:run": "docker run -p 3000:3000 observation-explorer",
    "docker:dev": "docker-compose --profile dev up --build",
    "docker:prod": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v --rmi all --remove-orphans",
    "test:docker": "docker-compose up --build --abort-on-container-exit",
    "test:build": "bun run build && echo 'Build test passed!'",
    "test:lint": "bun run lint && echo 'Lint test passed!'",
    "test:format": "bun run format:check && echo 'Format test passed!'",
    "test:all": "bun run test:format && bun run test:lint && bun run test:build",
    "health-check": "curl -f http://localhost:3000 || exit 1",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "deploy:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "deploy:stop": "docker-compose -f docker-compose.prod.yml down",
    "deploy:logs": "docker-compose -f docker-compose.prod.yml logs -f"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.60.1",
    "@opentelemetry/exporter-prometheus": "^0.202.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/semantic-conventions": "^1.34.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/mapbox-gl": "^3.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.525.0",
    "mapbox-gl": "^3.13.0",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "observation-js": "^1.15.0",
    "prom-client": "^15.1.3",
    "react": "^19.0.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.59.0",
    "react-map-gl": "^8.0.4",
    "react-resizable-panels": "^3.0.3",
    "recharts": "^3.0.2",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.7",
    "@tailwindcss/postcss": "^4",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "prettier": "^3.4.2",
    "semantic-release": "^23.0.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
